public class Punto3 {

	public static int diferencia (int [] arrayA) {
		int i;
		
			for ( i = 0; i < arrayA.length; i++) {
				if (arrayA[i] != 0) {
					arrayB[i]=arrayA[i]-arrayA[i+1];
					
				}
			
			
		}
		
		
		
		return arrayB[i];
	}
	
}

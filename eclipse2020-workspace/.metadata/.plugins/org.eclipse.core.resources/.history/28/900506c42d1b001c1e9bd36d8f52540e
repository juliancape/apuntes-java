package proyectoAjedrez;

public class Operacion {

	private String[][] tablero= new String[8][8];
	
	public void imprimir() {
		for (int i = 0; i < tablero.length; i++) {
			for (int j = 0; j < tablero.length; j++) {
				System.out.printf("%d", tablero[i][j]);
				System.out.print("\n");
			}
		}
		
		
		
	}
	
}

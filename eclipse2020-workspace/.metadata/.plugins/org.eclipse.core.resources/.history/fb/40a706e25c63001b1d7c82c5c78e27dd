public class Punto3 {

	public static int diferencia (int [] arrayA) {
		int i;
		
			for ( i = 0; i < arrayA.length; i++) {
				
					arrayA[i]=arrayA[i]-arrayA[i+1];
					
				
			return arrayA[i];
			
		}
		
		
		
		
	}
	
}
